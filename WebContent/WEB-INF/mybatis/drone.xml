<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="drone">

	<insert id="insert" parameterType="com.kosateam2.chicken.dto.DroneMission">
		insert into dronemissions
		values (#{oid}, #{seq}, #{command}, #{param1}, #{param2}, #{param3}, #{param4}, #{x}, #{y}, #{z})
	</insert>
</mapper>